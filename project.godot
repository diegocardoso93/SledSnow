config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[android]

modules="org/godotengine/godot/GodotAds,org/godotengine/godot/GDUnityAds"

[application]

config/name="Sled Snow"
run/main_scene="res://scenes/gamestart.tscn"
config/icon="res://icon.png"

[display]

window/size/width=600
window/size/height=1024
window/size/resizable=false
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"
window/handheld/emulate_touchscreen=true

[rendering]

environment/default_clear_color=Color( 1, 1, 1, 1 )
environment/default_environment="res://default_env.tres"
